@model Inmobiliaria_Avgustin.Models.Propietario

<h2>Editar Propietario</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />

    <div>
        <label>Nombre</label>
        <input asp-for="Nombre" />
        <span asp-validation-for="Nombre"></span>
    </div>

    <div>
        <label>Apellido</label>
        <input asp-for="Apellido" />
        <span asp-validation-for="Apellido"></span>
    </div>

    <div>
        <label>DNI</label>
        <input asp-for="Dni" />
        <span asp-validation-for="Dni"></span>
    </div>

    <div>
        <label>Tel√©fono</label>
        <input asp-for="Telefono" />
        <span asp-validation-for="Telefono"></span>
    </div>

    <div>
        <label>Email</label>
        <input asp-for="Email" />
        <span asp-validation-for="Email"></span>
    </div>

    <button type="submit">Guardar</button>
</form>
